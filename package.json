{
  "name": "manus-ai-agent",
  "version": "1.0.0",
  "description": "A Manus-style AI agent built with LangGraph that autonomously searches, crawls, and analyzes weather information",
  "main": "index.js",
  "scripts": {
    "install:all": "npm run install:backend && npm run install:frontend",
    "install:backend": "cd backend && npm install",
    "install:frontend": "cd frontend && npm install",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd frontend && npm start",
    "start": "concurrently \"npm run start:backend\" \"npm run start:frontend\"",
    "start:backend": "cd backend && npm start",
    "start:frontend": "cd frontend && npm start",
    "build": "npm run build:frontend",
    "build:frontend": "cd frontend && npm run build",
    "test": "npm run test:frontend",
    "test:frontend": "cd frontend && npm test"
  },
  "keywords": [
    "ai-agent",
    "langgraph",
    "weather",
    "manus",
    "autonomous",
    "puppeteer",
    "grok",
    "chatgpt"
  ],
  "author": "Manus AI Agent Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/manus-ai-agent.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/manus-ai-agent/issues"
  },
  "homepage": "https://github.com/yourusername/manus-ai-agent#readme",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
